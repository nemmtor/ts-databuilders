{
  "name": "playground",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun --console-depth 10 src/main.ts",
    "format": "biome format",
    "format:fix": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "check-actions": "pnpm exec biome check --formatter-enabled=false --linter-enabled=false",
    "check-actions:fix": "pnpm exec biome check --formatter-enabled=false --linter-enabled=false --write",
    "check-types": "tsc -b tsconfig.json",
    "check-knip": "knip"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@effect/language-service": "^0.44.0",
    "@effect/platform-bun": "^0.81.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.3.0",
    "dependency-cruiser": "^17.0.2",
    "knip": "^5.64.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@effect/cli": "^0.71.0",
    "@effect/platform": "^0.92.1",
    "effect": "^3.18.4",
    "ts-morph": "^27.0.2"
  }
}
